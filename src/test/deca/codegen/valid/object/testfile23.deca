class A{
    int x;
    int y;
    
    void setX(int x){
        this.x = x;
    }
    void range(int n,int i){
        
        while(i<n && true && true && true){
            setX(i);
            println(this.x);
            i = i+1;
        }
    }
}
class B extends A{
    
}
{
    B obj = new B();
    obj.setX(14);
    println(obj.x);
    obj.y = 21;
    println(obj.y);
    obj.range(10,6);
}

    