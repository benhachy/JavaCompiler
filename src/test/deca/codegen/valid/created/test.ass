; Initialisation des champs de A
init.A:
	TSTO #3
	BOV pile_pleine
	LOAD -2(LB), R1
	LOAD #4, R0
	STORE R0, 1(R1)
	LOAD #1, R0
	STORE R0, 2(R1)
