; Initialisation des champs de B
init.B:
	TSTO #4
	BOV pile_pleine
	LOAD -2(LB), R1
	LOAD #0, R0
	STORE R0, 3(R1)
	LOAD #0, R0
	STORE R0, 4(R1)
	LOAD #0, R0
	STORE R0, 5(R1)
; Appel de l'initialisation des champs hérités de A
	PUSH R1
	BSR init.A
	SUBSP #1
	RTS
code.B.addAtt1:
	TSTO #2	; test de debordement de la pile
	BOV pile_pleine
	PUSH R2
	PUSH R3
	LOAD -2(LB), R2
	LOAD -2(LB), R3
	LOAD #5, R2
fin.B.addAtt1:
; Restauration des registres
	POP R3
	POP R2
	RTS
; Initialisation des champs de C
init.C:
	TSTO #3
	BOV pile_pleine
	LOAD -2(LB), R1
	LOAD #0, R0
	STORE R0, 1(R1)
	STORE R0, 2(R1)
