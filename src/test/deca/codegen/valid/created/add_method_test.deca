class P {
    int x;
    int y=2;
    int getX(){
        return x;
    }
    void move(int a,int b,int c) {
        x = x + a-b-c;
        y = y * b;
    }
}
{
    P monTest = new P();
    monTest.move(10,5,2);
    println(monTest.x);
    println(monTest.y);
    monTest.x=monTest.y+5;
    println(monTest.x);
    println(monTest.getX());
}