// Description:
//  declaration de deux variables booleennes une initialisee par true
//  et l'autre par false
// Resultats:
//  1001
// 
// Historique:
//    cree le 16/01/2022
{
    boolean a =true;
    boolean b =false;
    if(a || !b){
        print(1);
    }
    if(!(!a || b)){
        print(0);
    }
    if(!a || !b){
        print(0);
    }
    if(!!(!!a || !!b)){
        print(1);
    }
}